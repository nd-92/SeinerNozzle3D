FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application         buiTurbFoam;

startFrom           latestTime;

startTime           0.3;

stopAt              endTime;

endTime             0.4;

deltaT              4e-8;

writeControl        timeStep;

writeInterval       25;

purgeWrite          3;

writeFormat         binary;

writePrecision      15;

writeCompression    off;

timeFormat          general;

timePrecision       9;

runTimeModifiable   false;

adjustTimeStep      no;

maxCo               1;

maxDeltaT           4e-8;

cwipiSwitch         false;

cwipiAveraging      false;

cwipiDim            3;

cwipiStep           1;

cwipiLambVector     true;

cwipiEntropy        true;

cwipiDsDt           true;

smoothenSources     true;

smoothingScale      1000000.0;

